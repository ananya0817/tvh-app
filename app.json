{
  "expo": {
    "name": "tvh-app",
    "slug": "tvh-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-Photoroom.png",
    "scheme": "com.supabase",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-Photoroom.png",
      "resizeMode": "contain",
      "backgroundColor": "#8d7a8e"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-Photoroom.png",
        "backgroundColor": "#8d7a8e"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
